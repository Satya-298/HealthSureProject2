<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee /WEB-INF/schema/web-facesconfig_2_0.xsd"
	version="2.0" metadata-complete="false">
	<!-- This file must be present with our jar, even if it is empty. Otherwise, 
		our Java annotations won't get scanned! -->

	<!-- Doctor Availability Beans -->

	<managed-bean>
		<managed-bean-name>availabilityDao</managed-bean-name>
		<managed-bean-class>com.infinite.jsf.provider.daoImpl.DoctorAvailabilityDaoImpl
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>availability</managed-bean-name>
		<managed-bean-class>com.infinite.jsf.provider.model.DoctorAvailability
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>totalSlots</property-name>
			<value>1</value>
		</managed-property>
	</managed-bean>


	<managed-bean>
		<managed-bean-name>availabilityController</managed-bean-name>
		<managed-bean-class>com.infinite.jsf.provider.controller.DoctorAvailabilityController
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>

		<managed-property>
			<property-name>availabilityDao</property-name>
			<property-class>com.infinite.jsf.provider.daoImpl.DoctorAvailabilityDaoImpl
			</property-class>
			<value>#{availabilityDao}</value>
		</managed-property>

		<managed-property>
			<property-name>availability</property-name>
			<property-class>com.infinite.jsf.provider.model.DoctorAvailability
			</property-class>
			<value>#{availability}</value>
		</managed-property>
	</managed-bean>


	<!-- Medical Procedure Beans -->

	<managed-bean>
		<managed-bean-name>medicalHistoryDao</managed-bean-name>
		<managed-bean-class>com.infinite.jsf.provider.daoImpl.MedicalHistoryDaoImpl
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>medicalProcedure</managed-bean-name>
		<managed-bean-class>com.infinite.jsf.provider.model.MedicalProcedure
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>medicalHistoryController</managed-bean-name>
		<managed-bean-class>com.infinite.jsf.provider.controller.MedicalHistoryController
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>

		<managed-property>
			<property-name>medicalHistoryDao</property-name>
			<property-class>com.infinite.jsf.provider.daoImpl.MedicalHistoryDaoImpl
			</property-class>
			<value>#{medicalHistoryDao}</value>
		</managed-property>

		<managed-property>
			<property-name>medicalProcedure</property-name>
			<property-class>com.infinite.jsf.provider.model.MedicalProcedure
			</property-class>
			<value>#{medicalProcedure}</value>
		</managed-property>
	</managed-bean>

	<!-- Appointment Beans -->

	<managed-bean>
		<managed-bean-name>appointmentDao</managed-bean-name>
		<managed-bean-class>com.infinite.jsf.provider.daoImpl.AppointmentDaoImpl
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

