	<managed-bean>
		<managed-bean-name>appointmentController</managed-bean-name>
		<managed-bean-class>com.infinite.jsf.provider.controller.AppointmentController
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>

		<managed-property>
			<property-name>appointmentDao</property-name>
			<property-class>com.infinite.jsf.provider.dao.AppointmentDao
			</property-class>
			<value>#{appointmentDao}</value>
		</managed-property>

		<!-- <managed-property> <property-name>appointment</property-name> <property-class>com.infinite.jsf.provider.model.Appointment</property-class> 
			<value>#{appointment}</value> </managed-property> -->
